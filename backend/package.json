{
    "name": "backend",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "console": "ts-node -r tsconfig-paths/register src/console.ts"
    },
    "dependencies": {
        "@aginix/nestjs-firebase-admin": "^2.1.1",
        "@azure/storage-blob": "^12.5.0",
        "@nestjs/common": "^7.6.15",
        "@nestjs/config": "^0.6.3",
        "@nestjs/core": "^7.6.15",
        "@nestjs/mapped-types": "*",
        "@nestjs/mongoose": "^7.2.4",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/platform-socket.io": "^8.0.0-alpha.3",
        "@nestjs/schedule": "^0.4.3",
        "@nestjs/websockets": "^7.6.15",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "dayjs": "^1.10.4",
        "firebase-admin": "^9.6.0",
        "mongoose": "^5.12.3",
        "mongoose-hidden": "^1.9.1",
        "mongoose-lean-virtuals": "^0.7.6",
        "passport": "^0.4.1",
        "passport-cookie": "^1.0.9",
        "passport-http-bearer": "^1.0.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.6",
        "socket.io-redis": "^6.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.0",
        "@nestjs/testing": "^7.6.15",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.22",
        "@types/multer": "^1.4.5",
        "@types/node": "^14.14.37",
        "@types/passport": "^1.0.6",
        "@types/passport-http-bearer": "^1.0.36",
        "@types/purdy": "^3.5.0",
        "@types/redis": "^2.8.28",
        "@types/socket.io": "^2.1.13",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "await-outside": "^3.0.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "purdy": "^3.5.1",
        "repl": "^0.1.3",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "moduleDirectories": [
            "node_modules",
            "src",
            "."
        ],
        "testEnvironment": "node"
    }
}
